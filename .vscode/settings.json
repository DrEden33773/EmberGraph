{
    "cSpell.words": [
        "ahash",
        "appender",
        "cpus",
        "dashmap",
        "eids",
        "Itertools",
        "ldbc",
        "oneshot"
    ],
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "always",
            "source.fixAll": "always"
        },
        "editor.tabSize": 2
    }
}